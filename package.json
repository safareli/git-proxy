{
  "name": "git-proxy",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build --compile --outfile git-proxy ./src/index.ts",
    "up": "docker compose down && docker compose up",
    "upd": "docker compose up --detach",
    "upf": "docker compose down -v  && docker compose up --rebuild",
    "typecheck": "tsgo",
    "test": "bun test"
  },
  "dependencies": {
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bun": "^1.2.4",
    "@typescript/native-preview": "^7.0.0-dev.20260127.1"
  }
}
